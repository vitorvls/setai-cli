{
  "name": "@setai/cli",
  "version": "0.1.1",
  "description": "CLI Tool to generate .cursor configuration structure for AI-assisted development",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "setai": "./dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "cli": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:docs": "vitest run docs/.vitepress/__tests__",
    "test:manual": "node scripts/test-manual.js",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "docs": "vitepress dev docs",
    "prepublishOnly": "npm run build && npm run test",
    "prepack": "npm run build"
  },
  "keywords": [
    "cli",
    "cursor",
    "ai",
    "development",
    "tool",
    "generator",
    "configuration",
    "assistant",
    "automation",
    "ide",
    "vscode",
    "jetbrains"
  ],
  "author": "vitorvls",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vitorvls/setai-cli.git"
  },
  "homepage": "https://github.com/vitorvls/setai-cli#readme",
  "bugs": {
    "url": "https://github.com/vitorvls/setai-cli/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "templates",
    "locales"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google/generative-ai": "^0.24.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^12.0.1",
    "openai": "^6.15.0",
    "ora": "^8.1.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitepress": "^1.6.4",
    "vitest": "^2.1.8"
  }
}
